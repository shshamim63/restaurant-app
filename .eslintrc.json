{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "import/extensions": 0,
        "no-tabs": 0,
        "indent": 0,
        "function-paren-newline": 0,
        "comma-dangle": 0,
        "implicit-arrow-linebreak": 0
    }
}